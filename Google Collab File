        "tril_example = torch.tril(torch.ones(block_length, block_length))\n",
        "wei_example = torch.zeros(block_length, block_length)\n",
        "wei_example = wei_example.masked_fill(tril_example == 0, float('-inf'))\n",
        "wei_example = F.softmax(wei_example, dim=-1)\n",
        "xbow = wei_example @ x\n",
        "\n",
        "# Create lower triangular matrix tril\n",
        "T = block_length\n",
        "tril = torch.tril(torch.ones(T, T))\n",
        "\n",
        "# Initialize wei with zeros and apply masked_fill\n",
        "wei = torch.zeros(T, T)\n",
        "wei = wei.masked_fill(tril == 0, float('-inf'))\n",
        "\n",
        "# Apply softmax along the last dimension\n",
        "wei = F.softmax(wei, dim=-1)\n",
        "xbow3 = wei @ x\n",
        "torch.allclose(xbow, xbow3)"
